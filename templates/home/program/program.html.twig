<div class="background">

           <div class="carousel-slider" id="responsive">
                    <h1 style="color:#fff;"><img src="/img/icon/partenaires.png"/>Scène Pop Folk</h1>
                        <div class="carousel-content" data-grid='{"xl":1,"lg":1,"md":1,"sm":1}' data-pagination="true">

                                <aside class="slide-item" style="">
                                        <table class="table-friday">
                                            <caption style="font-weight: bold; padding: 20px 0px;">Vendredi</caption>
                                                  <tr>
                                                    <th>Artistes</th>
                                                    <th>Heure</th>
                                                  </tr>
                            {% for artist in artistpopfriday %}

                                                  <tr>
                                                    <td><p><a href="{{ path('artist_show', {'name': artist.name}) }}">{{ artist.name }}</a></p></td>
                                                    <td><p>{{ artist.hour }}</p></td>
                                                  </tr>                                
                                
                            {% endfor %}
                            </table>        
                            </aside>
